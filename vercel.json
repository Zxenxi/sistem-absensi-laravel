{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "@vercel/php@latest"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "installCommand": "composer install --no-dev && npm install",
    "buildCommand": "npm run build"
}
